
@model BlueMine.Models.Project.NewItemModel

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers



@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <meta charset="utf-8" />

    <meta http-equiv="cache-control" content="max-age=0" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
    <meta http-equiv="pragma" content="no-cache" />

    <title>New Item</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.00, minimum-scale=1.00, maximum-scale=1.00" />

    <!--
    <link rel="stylesheet" type="text/css" href="ruler.min.css">

    <script src="ruler.js"></script>
    -->

    <style type="text/css">
        html, body 
        {
            margin: 0;
            padding: 0;
            width: 100%;
            height: 100%;
        }
        
        
        ul 
        {
            list-style-type: none;
        }
    </style>
    
    <!--
    <script src="@Url.Content("/GeneratedScripts/AsyncAjax.js")"></script>
    -->
    
</head>
<body>
    <h1>Hello New Item</h1>
    
    <label for="issue_tracker_id">Tracker<span class="required"> *</span></label>
    <select id="issue_tracker_id" name="issue[tracker_id]" onchange="console.log('used to updateIssueFrom')" asp-items="Model.Trackers">
        <option>--- Please select ---</option>
    </select>


    <label for="issue_subject">Thema<span class="required"> *</span></label>
    <input id="issue_subject" name="issue[subject]" size="80" maxlength="255" type="text" value="" >

    <label for="issue_description">Beschreibung</label>
    <textarea id="issue_description" name="issue[description]" cols="60" rows="10" accesskey="e" class="wiki-edit" ></textarea>
    
    <label for="issue_status_id">Status<span class="required"> *</span></label>
    <select id="issue_status_id" name="issue[status_id]" onchange="console.log('used to updateIssueFrom')" asp-items="Model.Stati" >
        <option>--- Please select ---</option>
    </select>

    <label for="issue_priority_id">Priorität<span class="required"> *</span></label>
    <select name="issue[priority_id]" id="issue_priority_id" asp-items="Model.Priorities">
    </select>


    <label for="issue_assigned_to_id">Zugewiesen an</label>
    <select name="issue[assigned_to_id]" id="issue_assigned_to_id">
        <option value="">&nbsp;</option>
        <option value="5">&lt;&lt; ich &gt;&gt;</option>
        <option value="1">COR Administrator</option>
        <option value="13">Fabio Visconti</option>
        <option value="6">Hanspeter Hafner</option>
        <option value="10">Josef Zihlmann</option>
        <option value="7">Nando Hämmerli</option>
        <option value="9">Patrick Sprenger</option>
        <option value="8">Rico Luder</option>
        <option value="14">Service Desk</option>
        <option value="5">Stefan Steiger</option>
        <option value="12">Thomas Hartmann</option>
    </select>



    <label for="issue_parent_issue_id">Übergeordnete Aufgabe</label>
    <input size="10" type="text" name="issue[parent_issue_id]" id="issue_parent_issue_id" class="ui-autocomplete-input autocomplete" autocomplete="off">


    <label for="issue_start_date">Beginn</label>
    <input size="10" type="text" name="issue[start_date]" id="issue_start_date" class="date hasDatepicker">


    <label for="issue_due_date">Abgabedatum</label>
    <input size="10" type="text" name="issue[due_date]" id="issue_due_date" class="date hasDatepicker">

    <label for="issue_estimated_hours">Geschätzter Aufwand</label>
    <input size="3" type="text" name="issue[estimated_hours]" id="issue_estimated_hours">

    <label for="issue_done_ratio">% erledigt</label>
    <select name="issue[done_ratio]" id="issue_done_ratio">
        <option selected="selected" value="0">0 %</option>
        <option value="10">10 %</option>
        <option value="20">20 %</option>
        <option value="30">30 %</option>
        <option value="40">40 %</option>
        <option value="50">50 %</option>
        <option value="60">60 %</option>
        <option value="70">70 %</option>
        <option value="80">80 %</option>
        <option value="90">90 %</option>
        <option value="100">100 %</option>
    </select>


    <p>
        <label for="issue_custom_field_values_3">
        <span title="verrechenbar Ja oder Nein" class="field-description">verrechenbar</span> 
        <span class="required">*</span></label>
        <span class="list_cf check_box_group">
            <label><input type="radio" id="issue_custom_field_values_3" name="issue[custom_field_values][3]" value="Ja"> Ja</label>
            <label><input type="radio" name="issue[custom_field_values][3]" value="Nein" checked="checked"> Nein</label>
        </span>
    </p>


    <label for="issue_custom_field_values_1"><span>gemeldet von</span></label>
    <input type="text" id="issue_custom_field_values_1" name="issue[custom_field_values][1]" value="" class="string_cf">


    <label for="issue_custom_field_values_2">
    <span title="Kundenname oder Intern" class="field-description">Kundenname</span> <span class="required">*</span>
    </label>
    <select name="issue[custom_field_values][2]" id="issue_custom_field_values_2" class="list_cf">
        <option value="">--- Bitte auswählen ---</option>
        <option value="INTERN">INTERN</option>
        <option value="------">------</option>
        <option value="BKB">BKB</option>
        <option value="Campus Sursee">Campus Sursee</option>
        <option value="Helvetia">Helvetia</option>
        <option value="Julius Bär">Julius Bär</option>
        <option value="Post">Post</option>
        <option value="Raiffeisen">Raiffeisen</option>
        <option value="Rockwell">Rockwell</option>
        <option value="RSI">RSI</option>
        <option value="SauterFM">SauterFM</option>
        <option value="SNB">SNB</option>
        <option value="Sonova (Phonak)">Sonova (Phonak)</option>
        <option value="SRGSSR">SRGSSR</option>
        <option value="Swisscom">Swisscom</option>
        <option value="Swisslife">Swisslife</option>
        <option value="SwissRe">SwissRe</option>
        <option value="Wincasa">Wincasa</option>
        <option value="Zürich">Zürich</option>
    </select>


    <p>
        <label for="issue_custom_field_values_5">
        <span title="Sollen Kunden über dieses Ticket beim nächsten Update informiert werden?" class="field-description">
            Kunden informieren
        </span>
        </label>
        <select name="issue[custom_field_values][5]" id="issue_custom_field_values_5" class="list_cf">
            <option value="">&nbsp;</option>
            <option selected="selected" value="--- Bitte auswählen ---">--- Bitte auswählen ---</option>
            <option value="Ja">Ja</option>
            <option value="Nein">Nein</option>
        </select>
    </p>



    <script type="text/javascript">
        
        function onDomReady()
        {
            console.log("dom ready");
        }
        
        if (document.addEventListener) document.addEventListener("DOMContentLoaded", onDomReady, false);
        else if (document.attachEvent) document.attachEvent("onreadystatechange", onDomReady);
        else window.onload = onDomReady;
        
        function onPageLoaded()
        {
            console.log("page loaded");
        }

        if (window.addEventListener) window.addEventListener("load", onPageLoaded, false);
        else if (window.attachEvent) window.attachEvent("onload", onPageLoaded);
        else window.onload = onPageLoaded;

    </script>

</body>
</html>
